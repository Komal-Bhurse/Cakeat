    {
        "version": 2,
        "builds": [
          {
            "src": "./server/index.js",
            "use": "@vercel/node"
          },
          {
            "src": "./client/package.json",
            "use": "@vercel/static-build"
          }
        ],
        "routes": [
          {
            "src": "/api/.*",
            "dest": "./server/index.js"
          },
          {
            "src": "/(.*)",
            "dest": "./client/$1",
            "headers": {
              "Cache-Control": "public, max-age=31536000, immutable"
            }
          }
        ]
      }
      
